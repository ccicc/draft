{
    "presets": [
        [ "env", {
            "targets": {
                "browser": [ "> 1 in CN", "ie >= 11" ],
                "node": "current"
            },
            "loose": true
        } ],
        "react",
        "stage-0"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": true,
            "polyfill": true,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        ["import",{
            "libraryName": "antd",
            "style": "css"
        }]
    ]
}